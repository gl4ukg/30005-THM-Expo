{
  "expo": {
    "name": "THM",
    "slug": "thm-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tess-icon-thm_white-on-green.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "appleTeamId": "55NT296GTJ",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "no.tess.thm-mobile",
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "package": "no.tess.thm_mobile",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.NFC",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#009640",
      "barStyle": "dark-content",
      "translucent": true,
      "hidden": false
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tess-icon-thm_white.png",
          "imageWidth": 1024,
          "resizeMode": "contain",
          "backgroundColor": "#009640"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to document hoses and inspect them.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/open-sans/OpenSans_400Regular.ttf",
            "node_modules/@expo-google-fonts/open-sans/OpenSans_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/roboto/Roboto_400Regular.ttf",
            "node_modules/@expo-google-fonts/roboto/Roboto_500Medium.ttf",
            "node_modules/@expo-google-fonts/roboto-condensed/RobotoCondensed_300Light.ttf",
            "node_modules/@expo-google-fonts/roboto-condensed/RobotoCondensed_400Regular.ttf"
          ]
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera.",
          "enableCodeScanner": true
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      [
        "react-native-nfc-manager",
        {
          "nfcPermission": "$(PRODUCT_NAME) needs access to your NFC reader.",
          "selectIdentifiers": ["A0000002471001"],
          "systemCodes": ["8008"],
          "includeNdefEntitlement": false
        }
      ],
      "expo-background-task",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "10ceefff-1bf3-4ebf-801a-ba18eb6ee142"
      }
    },
    "owner": "tess-as"
  }
}
